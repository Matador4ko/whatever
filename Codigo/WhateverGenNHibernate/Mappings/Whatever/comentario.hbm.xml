<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="WhateverGenNHibernate"
		                   namespace="WhateverGenNHibernate.EN.Whatever">
		    
		    
			
				<class name="ComentarioEN" table="Comentario"> 
				  	
						<id name="ID" column="ID" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Texto" type="string"> 
						<column name="texto"  not-null="true"/>
					</property>
					<property name="Creador" type="string"> 
						<column name="creador"  not-null="true"/>
					</property>
			
			
			
			
			
		  			
						<many-to-one name="Comentario_usuario" class="WhateverGenNHibernate.EN.Whatever.UsuarioEN" >	
								<column name="FK_ID_idUsuario"/> 
						</many-to-one>
											 					
			
		  			
						<many-to-one name="Comentario_evento2" class="WhateverGenNHibernate.EN.Whatever.EventoEN" >	
								<column name="FK_ID_idComentario_evento2"/> 
						</many-to-one>
											 					
			
		  			
						<many-to-one name="Comentario_reto" class="WhateverGenNHibernate.EN.Whatever.RetoEN" >	
								<column name="FK_id_idComentario_0"/> 
						</many-to-one>
											 					
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="ComentarioENReadAllHQL">from ComentarioEN</query>
		  <query name="ComentarioENreadFilterHQL"><![CDATA[FROM ComentarioEN]]></query>
		</hibernate-mapping>
	