<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="WhateverGenNHibernate"
		                   namespace="WhateverGenNHibernate.EN.Whatever">
		    
		    
			
				<class name="EventoEN" table="Evento"> 
				  	
						<id name="ID" column="ID" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Titulo" type="string"> 
						<column name="Titulo"  not-null="true"/>
					</property>
					<property name="Descripcion" type="string"> 
						<column name="descripcion"  not-null="true"/>
					</property>
					<property name="Fecha" type="date"> 
						<column name="fecha"  not-null="true"/>
					</property>
					<property name="Precio" type="integer"> 
						<column name="precio"  not-null="true"/>
					</property>
			
			
			
			
			
			
			
		  			
						<many-to-one name="Usuario_evento" class="WhateverGenNHibernate.EN.Whatever.UsuarioEN" >	
								<column name="FK_ID_idUsuario_evento"/> 
						</many-to-one>
											 					
			
		  			
						<bag name="Puntuacion_evento2" >
							<key>
							
								
								<column name="FK_ID_idEvento_puntuacion"/>
					      	</key>
							<one-to-many class="WhateverGenNHibernate.EN.Whatever.PuntuacionEN"/>
					    </bag>
					
			
		  			
						<bag name="Evento_mapa" >
							<key>
							
								
								<column name="FK_ID_idEvento_mapa2"/>
					      	</key>
							<one-to-many class="WhateverGenNHibernate.EN.Whatever.MapaEN"/>
					    </bag>
					
			
		  			
						<bag name="Comentario_evento" >
							<key>
							
								
								<column name="FK_ID_idComentario_evento2"/>
					      	</key>
							<one-to-many class="WhateverGenNHibernate.EN.Whatever.ComentarioEN"/>
					    </bag>
					
			
		  			
						<bag name="Reporte_evento2" >
							<key>
							
								
								<column name="FK_ID_idReporte"/>
					      	</key>
							<one-to-many class="WhateverGenNHibernate.EN.Whatever.ReporteEN"/>
					    </bag>
					
					
					
					
					
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="EventoENReadAllHQL">from EventoEN</query>
		  <query name="EventoENfiltrarPorUsuarioHQL"><![CDATA[FROM EventoEN as ev, UsuarioEN as us WHERE us.ID=:id_usu]]></query>
		</hibernate-mapping>
	