<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="WhateverGenNHibernate"
		                   namespace="WhateverGenNHibernate.EN.Whatever">
		    
		    
			
				<class name="MapaEN" table="Mapa"> 
				  	
						<id name="Id" column="idMapa" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Latitud" type="double"> 
						<column name="latitud"  not-null="true"/>
					</property>
					<property name="Longitud" type="double"> 
						<column name="longitud"  not-null="true"/>
					</property>
					<property name="Zoom" type="integer"> 
						<column name="zoom"  not-null="true"/>
					</property>
			
			
		  			
						<many-to-one name="Evento_mapa2" class="WhateverGenNHibernate.EN.Whatever.EventoEN" >	
								<column name="FK_ID_idEvento_mapa2"/> 
						</many-to-one>
											 					
			
			
		  			
					
							   <one-to-one name="Paso_0" class="WhateverGenNHibernate.EN.Whatever.PasoEN" property-ref="Paso" />							
					 
						
			
			
			
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="MapaENReadAllHQL">from MapaEN</query>
		  <query name="MapaENfiltrarEventosHQL"><![CDATA[FROM MapaEN, EventoEN as ev WHERE ev.ID != -1]]></query>
		  <query name="MapaENFiltrarPorEventoHQL"><![CDATA[FROM MapaEN, EventoEN as ev WHERE ev.ID=:id_evento]]></query>
		  <query name="MapaENFiltrarPorPasoHQL"><![CDATA[FROM MapaEN, PasoEN as pa WHERE pa.ID=:id_paso]]></query>
		</hibernate-mapping>
	