<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="WhateverGenNHibernate"
		                   namespace="WhateverGenNHibernate.EN.Whatever">
		    
		    
			
				<class name="PuntuacionEN" table="Puntuacion"> 
				  	
						<id name="Id" column="idPuntuacion" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Puntuacion" type="integer"> 
						<column name="puntuacion"  not-null="true"/>
					</property>
			
			
		  			
						<many-to-one name="Usuario_puntuacion2" class="WhateverGenNHibernate.EN.Whatever.UsuarioEN" >	
								<column name="FK_ID_idUsuario_puntuacion2"/> 
						</many-to-one>
											 					
			
		  			
						<many-to-one name="Reto_puntuacion" class="WhateverGenNHibernate.EN.Whatever.RetoEN" >	
								<column name="FK_id_idReto"/> 
						</many-to-one>
											 					
			
		  			
						<many-to-one name="Evento_puntuacion" class="WhateverGenNHibernate.EN.Whatever.EventoEN" >	
								<column name="FK_ID_idEvento_puntuacion"/> 
						</many-to-one>
											 					
			
			
					
					
					
					
					
		
		 </class>	
		  
		  <query name="PuntuacionENReadAllHQL">from PuntuacionEN</query>
		  <query name="PuntuacionENfiltrarRetoHQL"><![CDATA[FROM PuntuacionEN,UsuarioEN as usu, RetoEN as re WHERE re.ID=:id_reto and usu.ID=:id_usuario]]></query>
		  <query name="PuntuacionENfiltrarEventoHQL"><![CDATA[FROM PuntuacionEN,UsuarioEN as usu, EventoEN as ev WHERE ev.ID=:id_evento and usu.ID=:id_usuario]]></query>
		  <query name="PuntuacionENfiltrarMediaEventoHQL"><![CDATA[FROM PuntuacionEN, EventoEN as ev WHERE ev.ID=:id_evento]]></query>
		  <query name="PuntuacionENfiltrarMediaRetoHQL"><![CDATA[FROM PuntuacionEN, RetoEN as re WHERE re.ID=:id_reto]]></query>
		  <query name="PuntuacionENfiltrarTodosEventosHQL"><![CDATA[FROM PuntuacionEN, EventoEN as ev WHERE ev.ID=:id_evento]]></query>
		  <query name="PuntuacionENfiltrarTodosRetosHQL"><![CDATA[FROM PuntuacionEN, RetoEN as re WHERE re.ID=:id_reto ]]></query>
		  <query name="PuntuacionENfiltrarPorUsuarioHQL"><![CDATA[FROM PuntuacionEN, UsuarioEN as us WHERE us.ID=:id_usuario]]></query>
		</hibernate-mapping>
	