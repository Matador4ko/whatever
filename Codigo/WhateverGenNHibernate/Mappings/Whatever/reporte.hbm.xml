<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="WhateverGenNHibernate"
		                   namespace="WhateverGenNHibernate.EN.Whatever">
		    
		    
			
				<class name="ReporteEN" table="Reporte"> 
				  	
						<id name="ID" column="ID" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Motivo" type="string"> 
						<column name="motivo"  not-null="true"/>
					</property>
			
			
			
			
		  			
						<many-to-one name="Usuario_reporte" class="WhateverGenNHibernate.EN.Whatever.UsuarioEN" >	
								<column name="FK_ID_idUsuario_reporte"/> 
						</many-to-one>
											 					
			
		  			
						<bag name="Admin_reporte" table="[admin_admin_0]"  >
							<key>
								
									<column name="FK_ID_idAdmin_reporte2"/> 
							</key>
      						<many-to-many class="WhateverGenNHibernate.EN.Whatever.AdminEN">
									<column name="FK_ID_idAdmin_reporte"/> 
      						</many-to-many>
    					</bag>
			
		  			
						<many-to-one name="Reporte_reto2" class="WhateverGenNHibernate.EN.Whatever.RetoEN" >	
								<column name="FK_id_idReporte_0"/> 
						</many-to-one>
											 					
			
		  			
						<many-to-one name="Reporte" class="WhateverGenNHibernate.EN.Whatever.EventoEN" >	
								<column name="FK_ID_idReporte"/> 
						</many-to-one>
											 					
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="ReporteENReadAllHQL">from ReporteEN</query>
		  <query name="ReporteENfiltrarEventoHQL"><![CDATA[FROM ReporteEN,UsuarioEN as usu, EventoEN as ev WHERE ev.ID=:id_evento and usu.ID=:id_usuario]]></query>
		  <query name="ReporteENfiltrarRetoHQL"><![CDATA[FROM ReporteEN,UsuarioEN as usu, RetoEN as re WHERE re.ID=:id_reto and usu.ID=:id_usuario]]></query>
		  <query name="ReporteENfiltrarTodosEventosHQL"><![CDATA[FROM ReporteEN, EventoEN as ev WHERE ev.ID=:id_evento]]></query>
		  <query name="ReporteENfiltrarTodosRetosHQL"><![CDATA[FROM ReporteEN, RetoEN as re WHERE re.ID=:id_reto]]></query>
		  <query name="ReporteENfiltrarPorUsuarioHQL"><![CDATA[FROM ReporteEN, UsuarioEN as us WHERE us.ID=:id_usuario]]></query>
		</hibernate-mapping>
	