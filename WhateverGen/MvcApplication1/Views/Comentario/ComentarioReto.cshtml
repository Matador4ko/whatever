@model IEnumerable<WhateverGenNHibernate.EN.Whatever.ComentarioEN>

@{
    ViewBag.Title = "ComentarioReto";
}

<h2>ComentarioReto</h2>


<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Texto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Creador)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Texto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Creador)
        </td>
        <td>
            @Html.ActionLink("Delete", "Delete", new { id = item.ID }) |
            @Html.ActionLink("Volver al Reto", "Details", "Reto", new { id = item.Reto.ID }, "null")
        </td>
    </tr>
}

</table>
