@model IEnumerable<WhateverGenNHibernate.EN.Whatever.PuntuacionEN>

@{
    ViewBag.Title = "PuntuacionGymkana";
}

<h2>PuntuacionGymkana</h2>


<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Puntuacion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Puntuacion)
        </td>
    </tr>
}

</table>
