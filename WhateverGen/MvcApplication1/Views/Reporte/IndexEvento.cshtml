@model IEnumerable<WhateverGenNHibernate.EN.Whatever.ReporteEN>

@{
    ViewBag.Title = "IndexEvento";
}

<h2>IndexEvento</h2>


<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Motivo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Motivo)
        </td>
        
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID }) |
            @Html.ActionLink("Volver al Evento", "Details", "Evento", new { id = item.Evento.ID },"null")


        </td>
    </tr>
}

</table>
