<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="WhateverGenNHibernate"
		                   namespace="WhateverGenNHibernate.EN.Whatever">
		    
		    
			
				<class name="ReporteEN" table="Reporte"> 
						<id name="ID" column="ID"/> 
			
					
					<property name="Motivo" type="string"> 
						<column name="motivo"  not-null="true"/>
					</property>
			
			
			
			
		  			
						<many-to-one name="Usuario_reporte" class="WhateverGenNHibernate.EN.Whatever.UsuarioEN" >	
								<column name="FK_ID_idUsuario_reporte"/> 
						</many-to-one>
											 					
			
		  			
						<bag name="Admin_reporte" table="[admin_admin_0]"  >
							<key>
								
									<column name="FK_ID_idAdmin_reporte2"/> 
							</key>
      						<many-to-many class="WhateverGenNHibernate.EN.Whatever.AdminEN">
									<column name="FK_ID_idAdmin_reporte"/> 
      						</many-to-many>
    					</bag>
			
		  			
						<bag name="Reporte_reto2" >
							<key>
							
								
								<column name="FK_ID_idReporte"/>
					      	</key>
							<one-to-many class="WhateverGenNHibernate.EN.Whatever.RetoEN"/>
					    </bag>
					
			
		  			
						<many-to-one name="Reporte" class="WhateverGenNHibernate.EN.Whatever.EventoEN" >	
								<column name="FK_ID_idReporte"/> 
						</many-to-one>
											 					
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="ReporteENReadAllHQL">from ReporteEN</query>
		</hibernate-mapping>
	